{
  "name": "webpack-static-sample",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --open --mode development --display-error-details --env.TARGET_ENV=local",
    "deploy:production": "npm-run-all --sequential clean build:production dist:production",
    "deploy:development": "npm-run-all --sequential clean build:development dist:development",
    "build:production": "webpack --env.TARGET_ENV=production",
    "build:development": "webpack --env.TARGET_ENV=development",
    "dist:development:": "aws s3  cp --recursive ./dist s3://dev.site.com",
    "dist:production:": "aws s3  cp --recursive ./dist s3://site.com",
    "clean": "rimraf ./dist"
  },
  "author": "Sebastian Pikand",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^3.2.0",
    "dotenv-webpack": "^2.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-link-type-plugin": "^1.0.3",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.10.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.0",
    "sass-resources-loader": "^2.0.1",
    "style-loader": "^1.0.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.11.0"
  }
}
